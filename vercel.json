{
    "version": 2,
    "buildCommand": "cd frontend && CI=false npm install --legacy-peer-deps && CI=false npm run build",
    "outputDirectory": "frontend/build",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}